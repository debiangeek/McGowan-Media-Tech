{
  "name": "mcgowan-media-tech",
  "version": "1.0.0",
  "description": "Resume site",
  "main": "server.js",
  "scripts": {
    "client-install": "cd mcgowanmediatech && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix mcgowanmediatech",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix mcgowanmediatech && npm run build --prefix mcgowanmediatech"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/debiangeek/McGowan-Media-Tech.git"
  },
  "author": "Mike McGowan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/debiangeek/McGowan-Media-Tech/issues"
  },
  "homepage": "https://github.com/debiangeek/McGowan-Media-Tech#readme",
  "dependencies": {
    "concurrently": "^4.1.0",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
}
